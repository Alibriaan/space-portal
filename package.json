    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "postcss-normalize": "^10.0.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
