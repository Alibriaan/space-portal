{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": 1
  },
  "overrides": [
    {
      "files": ["*.interfaces.ts"],
      "rules": {
        "@typescript-eslint/ban-types": 0
      }
    }
  ]
}